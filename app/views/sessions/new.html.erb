<h1>Login Here</h1>

<%= flash[:error] %>

<%= form_for(:user) do |f| %>
<div>
  <%= f.label :username %>
  <%= f.text_field :username %>
</div>
<br>
<div>
  <%= f.label :password %>
  <%= f.password_field :password %>
</div>
<br>
<div>
  <%= f.submit 'Login' %>
</div>
<% end %>
<br>
<div>
  <%= button_to "Login with Google", '/auth/google_oauth2' %>
</div>